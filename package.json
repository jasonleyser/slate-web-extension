{
  "name": "SlateforWeb",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.1",
    "@fontsource/inter": "^4.5.0",
    "File": "^0.10.2",
    "eslint-plugin-prettier": "^4.0.0",
    "html2canvas": "^1.3.2",
    "http2": "^3.3.7",
    "jquery": "3.4.1",
    "node-fetch": "^2.6.1",
    "npm-watch": "^0.11.0",
    "open-graph": "^0.2.6",
    "prettier": "^2.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "2.0.3",
    "react-hot-keys": "^2.6.2",
    "react-scripts": "5.0.0",
    "react-shadow": "^19.0.2",
    "react-shadow-root": "^6.1.0",
    "react-usestateref": "^1.0.8",
    "typeface-inter": "^3.18.1",
    "use-react-screenshot": "^1.0.2",
    "web-vitals": "^2.1.4"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "public"
      ],
      "extensions": "js,jsx"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env INLINE_RUNTIME_CHUNK=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch": "npm-watch"
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "browser": true,
      "webextensions": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "7.0.2",
    "eslint-config-prettier": "^8.3.0"
  }
}
